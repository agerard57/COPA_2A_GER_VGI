<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbMenu" prefHeight="405.0" prefWidth="658.0" styleClass="background" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerGabarit">
   <children>
      <AnchorPane fx:id="apMenu" prefHeight="19.0" prefWidth="640.0" styleClass="barre-menu">
         <children>
            <Separator layoutY="27.0" prefHeight="0.0" prefWidth="658.0" styleClass="separateur-menu" />
            <ImageView fx:id="btnQuit" fitHeight="21.0" fitWidth="25.0" layoutX="629.0" layoutY="4.0" onMouseClicked="#pageFermer" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../medias/quit.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
            <ImageView fx:id="btnClose" fitHeight="30.0" fitWidth="25.0" layoutX="602.0" layoutY="4.0" onMouseClicked="#pageReduire" pickOnBounds="true" preserveRatio="true" styleClass="img-close">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
            <ImageView fx:id="btnSettings" fitHeight="21.0" fitWidth="25.0" layoutX="33.0" layoutY="4.0" onMouseClicked="#pageSettings" pickOnBounds="true" preserveRatio="true" styleClass="img-settings">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
            <ImageView fx:id="btnHome" fitHeight="21.0" fitWidth="41.0" layoutX="3.0" layoutY="3.0" onMouseClicked="#pageHome" pickOnBounds="true" preserveRatio="true" styleClass="img-home">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
         </children>
         <cursor>
            <Cursor fx:constant="MOVE" />
         </cursor>
      </AnchorPane>
      <AnchorPane fx:id="apMenuPrincipal" prefHeight="409.0" prefWidth="658.0" />
   </children>
   <stylesheets>
      <URL value="@themeclair.css" />
      <URL value="@themesombre.css" />
   </stylesheets>
</VBox>
